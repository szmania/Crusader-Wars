    </EmbeddedResource>
    <EmbeddedResource Update="client\Options\UC_UnitsOptions.resx">
      <DependentUpon>UC_UnitsOptions.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="client\BETAUPDATE_Message.resx">
      <DependentUpon>BETAUPDATE_Message.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="client\UC_Toggle.resx">
      <DependentUpon>UC_Toggle.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="client\WarningMessage\WarningMessage.resx">
      <DependentUpon>WarningMessage.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="mod_manager\UC_UnitMapper.resx">
      <DependentUpon>UC_UnitMapper.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <None Update="Properties\Settings.settings">
      <Generator>SettingsSingleFileGenerator</Generator>
      <LastGenOutput>Settings.Designer.cs</LastGenOutput>
    </None>
  </ItemGroup>

