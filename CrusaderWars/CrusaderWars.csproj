<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net9.0-windows</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <AssemblyName>CrusaderWars</AssemblyName>
    <RootNamespace>CrusaderWars</RootNamespace>
    <ApplicationIcon>logo.ico</ApplicationIcon>
    <ProductName>Crusader Wars</ProductName>
    <PublisherName>Faray and Community</PublisherName>
    <StartupObject>CrusaderWars.Program</StartupObject>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="SharpZipLib" Version="1.4.2" />
    <PackageReference Include="System.Text.Json" Version="8.0.5" />
    <PackageReference Include="Microsoft.Windows.Compatibility" Version="9.0.0" />
    <PackageReference Include="System.Windows.Extensions" Version="9.0.0" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="client\MainFile.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="client\MainFile.Designer.cs">
      <DependentUpon>MainFile.cs</DependentUpon>
    </Compile>
    <Compile Update="client\LoadingScreen.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="client\LoadingScreen.Designer.cs">
      <DependentUpon>LoadingScreen.cs</DependentUpon>
    </Compile>
    <Compile Update="client\Options\Options.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="client\Options\Options.Designer.cs">
      <DependentUpon>Options.cs</DependentUpon>
    </Compile>
    <Compile Update="client\Options\UC_BattleScaleOptions.cs">
      <SubType>UserControl</SubType>
    </Compile>
    <Compile Update="client\Options\UC_BattleScaleOptions.Designer.cs">
      <DependentUpon>UC_BattleScaleOptions.cs</DependentUpon>
    </Compile>
    <Compile Update="client\Options\UC_GeneralOptions.cs">
      <SubType>UserControl</SubType>
    </Compile>
    <Compile Update="client\Options\UC_GeneralOptions.Designer.cs">
      <DependentUpon>UC_GeneralOptions.cs</DependentUpon>
    </Compile>
    <Compile Update="client\Options\UC_UnitsOptions.cs">
      <SubType>UserControl</SubType>
    </Compile>
    <Compile Update="client\Options\UC_UnitsOptions.Designer.cs">
      <DependentUpon>UC_UnitsOptions.cs</DependentUpon>
    </Compile>
    <Compile Update="client\BETAUPDATE_Message.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="client\BETAUPDATE_Message.Designer.cs">
      <DependentUpon>BETAUPDATE_Message.cs</DependentUpon>
    </Compile>
    <Compile Update="client\UC_Toggle.cs">
      <SubType>UserControl</SubType>
    </Compile>
    <Compile Update="client\UC_Toggle.Designer.cs">
      <DependentUpon>UC_Toggle.cs</DependentUpon>
    </Compile>
    <Compile Update="client\WarningMessage\WarningMessage.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="client\WarningMessage\WarningMessage.Designer.cs">
      <DependentUpon>WarningMessage.cs</DependentUpon>
    </Compile>
    <Compile Update="mod_manager\UC_UnitMapper.cs">
      <SubType>UserControl</SubType>
    </Compile>
    <Compile Update="mod_manager\UC_UnitMapper.Designer.cs">
      <DependentUpon>UC_UnitMapper.cs</DependentUpon>
    </Compile>
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
    <Compile Update="Properties\Settings.Designer.cs">
      <DesignTimeSharedInput>True</DesignTimeSharedInput>
      <AutoGen>True</AutoGen>
      <DependentUpon>Settings.settings</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="client\MainFile.resx">
      <DependentUpon>MainFile.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="client\LoadingScreen.resx">
      <DependentUpon>LoadingScreen.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="client\Options\Options.resx">
      <DependentUpon>Options.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="client\Options\UC_BattleScaleOptions.resx">
      <DependentUpon>UC_BattleScaleOptions.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="client\Options\UC_GeneralOptions.resx">
